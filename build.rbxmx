			resolve()
			--[[
				*
				* Updating the interface because
				* the ModuleScript is loaded with the last source code
			]]
			self:updateTree()
		end):catch(function(traceback)
			self:setState({
				traceback = traceback,
				status = "Error",
			})
		end)
	end
